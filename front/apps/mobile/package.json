		"react-native-svg": "15.2.0",
		"react-native-uuid": "^2.0.2",
		"react-native-video": "^6.4.3",
		"yoshiki": "1.2.14"
	},
	"expo": {
		"install": {
			"exclude": [
				"react-native"
			]
		}
	},
